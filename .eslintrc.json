{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars":  0,
        "no-empty": 0
    },
    "plugins": [
        "react"
    ],
    "env": {
         "browser": true,
         "node": true,
         "jquery": true
     }
}
